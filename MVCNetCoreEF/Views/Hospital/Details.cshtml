@model Hospital

<div class="container mt-5">
    <h1>Detalles Hospital</h1>
    <ul class="list-group">
        <li class="list-group-item">
            Id: @Model.IdHospital
        </li>
        <li class="list-group-item">
            Nombre: @Model.Nombre
        </li>
        <li class="list-group-item">
            Direccion: @Model.Direccion
        </li>
        <li class="list-group-item">
            Telefono: @Model.Telefono
        </li>
        <li class="list-group-item">
            Camas: @Model.Camas
        </li>
    </ul>
</div>